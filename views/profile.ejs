<!doctype html>
<html>
<head>
    <title>21 Savage Fan Site</title>
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css">
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
    <link rel="stylesheet" href="style.css">
</head>
<body>
<div class="container">

    <div class="page-header text-center">
        <h1><span class="fa fa-anchor"></span> Profile Page</h1>
        <a href="/logout" class="btn btn-default btn-sm">Logout</a>
    </div>

    <div class="row">


        
        <div class="col-sm-6">
          <h1>Spells</h1>
          <ul class="spellList">
          <% for(var i=0; i<spells.length; i++) {%>
            <li class="spellList">
              <h3 class="spellName"><%= spells[i].name %></h3>
              <p class="spellDescription"><%= spells[i].description %></p>
              <span class="wands"><%= spells[i].wand %></span>
              <span><i class="fa-solid fa-wand-sparkles"></i></span>
              <span><i class="fa-solid <%=spells[i].pinned ? 'fa-thumbtack-slash' : 'fa-thumbtack' %> pinned"></i></span>
              <span><i class="fa fa-trash-o" aria-hidden="true"></i></span>
            </li>
          <% } %>
          </ul>

        
          
            
            <button class="getSpell"type="submit">Get A Spell</button>
          
        </div>
        

</div>

<script src="https://kit.fontawesome.com/27754e0c49.js" crossorigin="anonymous"></script>
<script src="main.js"></script>

</body>
</html>
